{
  "extends": ["unobtrusive", "unobtrusive/import"],
  "env": {
    "browser": true,
    "node": true,
    "es6": true,
    "jest": true
  },
  "globals": {
    "nw": false,
    "globalThis": false
  },
  "overrides": [
    {
      "files": ["client/**/*.js"],
      "rules": {
        "import/no-extraneous-dependencies": "off"
      }
    }
  ]
}
