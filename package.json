{
  "name": "node-nw",
  "version": "0.5.0",
  "description": "Execute some node script in a headless nw.js instance",
  "main": "index.js",
  "bin": {
    "node-nw": "bin.js"
  },
  "scripts": {
    "test": "jest",
    "cli": "env DEBUG='node-nw:*' node bin.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/suchipi/node-nw.git"
  },
  "keywords": [
    "nw",
    "node-webkit"
  ],
  "author": "Lily Scott <me@suchipi.com> (http://suchipi.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/suchipi/node-nw/issues"
  },
  "homepage": "https://github.com/suchipi/node-nw#readme",
  "dependencies": {
    "debug": "^4.1.1",
    "mkdirp": "^1.0.3",
    "pipe-wrench": "^1.0.0",
    "rimraf": "^3.0.2",
    "shell-escape": "^0.2.0",
    "supports-color": "^3.2.3",
    "yargs-parser": "^4.2.1"
  },
  "optionalDependencies": {
    "@nwjs-binaries/osx-x64": "0.44.2-sdk",
    "@nwjs-binaries/linux-x64": "0.44.2-sdk",
    "@nwjs-binaries/win-x64": "0.44.2-sdk"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.8.3",
    "@babel/preset-env": "^7.8.4",
    "eslint": "^6.8.0",
    "eslint-config-unobtrusive": "^1.2.5",
    "eslint-plugin-import": "^2.20.1",
    "first-base": "^0.1.2",
    "jest": "^25.1.0",
    "prettier": "^1.19.1",
    "strip-ansi": "^6.0.0"
  },
  "jest": {
    "modulePathIgnorePatterns": [
      "<rootDir>/client/package.json"
    ],
    "testTimeout": 15000
  }
}
